                <script>
                    let tabsContainer = document.querySelector("#tabs");

                    let tabTogglers = tabsContainer.querySelectorAll("a");

                    tabTogglers.forEach(function(toggler) {
                        toggler.addEventListener("click", function(e) {
                            e.preventDefault();

                            let tabName = this.getAttribute("href");
                            addToQuery('tab',tabName.replace('#',''));

                            let tabContents = document.querySelector("#tab-contents");

                            for (let i = 0; i < tabContents.children.length; i++) {

                                tabTogglers[i].parentElement.classList.remove("text-blue-c100");
                                tabContents.children[i].classList.remove("hidden");
                                if ("#" + tabContents.children[i].id === tabName) {
                                    continue;
                                }
                                tabContents.children[i].classList.add("hidden");

                            }

                            tabTogglers.forEach(function(toggler) {
                                toggler.classList.remove("text-blue-c100");
                                toggler.querySelectorAll('path').forEach(function (path){
                                    path.classList.remove('fill-blue-c100');
                                })
                            });

                            e.target.parentElement.classList.add("text-blue-c100");
                            e.target.parentElement.querySelectorAll('path').forEach(function (path){
                                path.classList.add('fill-blue-c100');
                            })
                        });
                    });

                    var default_tab = getQuery('tab') || 'profile';
                    $(`[href="#${default_tab}"]`).get(0).click();
                    // document.getElementById("default-tab").click();

                </script>
