                <!-- end article -->

                {%if blog.has_comment%}
                    <!-- start comment box -->
                    <div class="hidden md:block mt-[22px] bg-white rounded-[10px] px-[15px] py-[20px] xl:flex xl:gap-4 xl:items-start">
                        <div class=" xl:w-[50%]">
                            {%if AuthUser%}
                                <form onsubmit="send_comment('{{CommentTypes.blog}}',this,'{{blog.id}}');return false;">
                                    <label for="commentText" class="font-shabnam-bold-digit text-[16px]">متن نظر:</label>
                                    <textarea name="text" id="commentText" rows="6" class="block border border-gray-c200 rounded-[10px] p-[10px] w-full h-[200px] xl:h-auto mt-[10px] input-nonOutline focus:border-blue-c200 font-shabnam-normal-digit text-[15px]"></textarea>
                                    <button type="submit" class="rounded-[10px] mt-[10px] mb-[10px] py-[12px] w-full bg-gradient-to-r from-blue-c100 to-blue-c200 hover:shadow-[0_0px_8px_1px_rgba(0,0,0,0.3)] transition-all duration-300 text-white" >ارسال نظر</button>
                                </form>
                            {%else%}
                                لطفا ابتدا وارد شوید
                            {%endif%}
                        </div>
                        <div class="bg-gray-c200 rounded-[10px] px-[22px] py-[25px] mt-[20px] xl:mt-0 xl:w-[50%]">
                            <p class="font-shabnam-bold-digit text-[16px] mb-[10px]">
                                {{blogSettings.web_blogPage_comments_title.value_web}}
                            </p>
                            <p class="font-shabnam-normal-digit text-[16px]">
                                {{blogSettings.web_blogPage_comments_text.value_web}}
                            </p>
                        </div>
                    </div>
                    <!-- end comment box -->
                    {%if blog.comments.length%}
                        <div class="bg-white rounded-[10px] px-[16px] py-[18px] mt-[10px] pb-[10px] ">
                                    
                            {%for comment in userComments%}
                                <div class="border-b border-gray-c200">
                                    {%render '/components/main/blog/comment',comment=comment,confirmed=false%}
                                </div>
                            {%endfor%}
                            
                            {%for comment in blog.comments%}
                                <div class="border-b border-gray-c200">
                                    {%render '/components/main/blog/comment', comment=comment,confirmed=true%}
                                </div>
                            {%endfor%}

