                        </div>
                    {%endif%}
                {%endif%}
            </div>

            {%render '/components/main/blog/newestBlogs'%}

        </div>

        {%if blog.related_blogs.length%}
            <!-- start other articles -->
            <div class="font-shabnam-bold-digit">
                {%letRoute blogs_route={routeName:"blogs",category=blog.category.seo.url}%}
                {%render '/components/commons/blogs/slider',blogs=blog.related_blogs,title="مقالات مرتبط",link=blogs_route,limit=10%}
            </div>
            <!-- end other articles -->
        {%endif%}

