                                        <form onsubmit="send_comment('{{CommentTypes.product}}',this,'{{product.id}}');return false;">
                                            <label for="commentText" class="relative font-shabnam-bold-digit text-[16px] flex justify-between">
                                                <span>متن نظر:</span>
                                                <span class="ml-[15px] md:hidden [&>*:nth-child(2)]:hover:opacity-100 [&>*:nth-child(2)]:hover:visible">
                                                    <svg class="!inline" width="26" height="26" viewBox="0 0 26 26" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path d="M12.9994 23.8334C14.4223 23.8353 15.8315 23.5559 17.1461 23.0113C18.4607 22.4668 19.6548 21.6678 20.6596 20.6603C21.6671 19.6555 22.4661 18.4615 23.0106 17.1469C23.5552 15.8323 23.8345 14.423 23.8327 13.0001C23.8345 11.5772 23.5551 10.1679 23.0106 8.85332C22.466 7.53872 21.6671 6.34469 20.6596 5.33984C19.6548 4.33236 18.4607 3.53338 17.1461 2.98884C15.8315 2.44429 14.4223 2.16491 12.9994 2.16676C11.5764 2.16494 10.1672 2.44434 8.85259 2.98888C7.53799 3.53342 6.34396 4.33238 5.33911 5.33984C4.33165 6.34469 3.53269 7.53872 2.98815 8.85332C2.4436 10.1679 2.16421 11.5772 2.16602 13.0001C2.16418 14.423 2.44356 15.8323 2.9881 17.1469C3.53265 18.4615 4.33162 19.6555 5.33911 20.6603C6.34396 21.6678 7.53799 22.4668 8.85259 23.0113C10.1672 23.5558 11.5764 23.8352 12.9994 23.8334V23.8334Z" stroke="#636363" stroke-width="2" stroke-linejoin="round"/>
                                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M12.9987 20.0418C13.3578 20.0418 13.7023 19.8992 13.9562 19.6452C14.2102 19.3912 14.3529 19.0468 14.3529 18.6877C14.3529 18.3285 14.2102 17.9841 13.9562 17.7301C13.7023 17.4762 13.3578 17.3335 12.9987 17.3335C12.6396 17.3335 12.2951 17.4762 12.0412 17.7301C11.7872 17.9841 11.6445 18.3285 11.6445 18.6877C11.6445 19.0468 11.7872 19.3912 12.0412 19.6452C12.2951 19.8992 12.6396 20.0418 12.9987 20.0418Z" fill="#636363"/>
                                                        <path d="M13 6.5V15.1667" stroke="#636363" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                                    </svg>
                                                    <span class="absolute left-[5px] top-[20px] opacity-0 invisible transition-all bg-gray-c100 rounded-[10px] px-[16px] py-[15px] md:py-[25px] mt-[20px] md:mr-[50px] before:absolute before:content-[url('/images/Polygon-3.png')] before:top-[-12px] before:left-[12px]">
                                                        <p class="font-shabnam-bold-digit text-[12px] mb-[10px]">
                                                            {{productSettings.web_productPage_comments_title.value_web}}
                                                        </p>
                                                        <p class="font-shabnam-normal-digit text-[12px] text-justify">
                                                            {{productSettings.web_productPage_comments_text.value_web}}
                                                        </p>
                                                    </span>
                                                </span>
                                            </label>
                                            <textarea name="text" id="commentText" rows="8" class="block border border-gray-c200 rounded-[10px] p-[10px] w-full mt-[10px] input-nonOutline focus:border-blue-c200 font-shabnam-normal text-[15px]"></textarea>
                                            <button type="submit" class="rounded-[10px] mt-[10px] mb-[10px] py-[12px] w-full bg-gradient-to-r from-blue-c100 to-blue-c200 hover:shadow-[0_0px_8px_1px_rgba(0,0,0,0.3)] transition-all duration-300 text-white" >ارسال نظر</button>
                                        </form>
                                    {%else%}
                                        لطفا ابتدا وارد شوید
                                    {%endif%}
                                </div>
                                <div class="hidden md:block bg-gray-c100 rounded-[10px] px-[22px] py-[25px] mt-[20px] md:mt-0 h-full">
                                    <p class="font-shabnam-bold-digit text-[13px] mb-[10px]">
                                        {{productSettings.web_productPage_comments_title.value_web}}
                                    </p>
                                    <p class="font-shabnam-normal-digit text-[14px] text-justify">
                                        {{productSettings.web_productPage_comments_text.value_web}}
                                    </p>
                                </div>
                            </div>
                            <!-- end comment box -->
                            <p class="font-shabnam-normal-digit text-[14px] mt-[5px]">
                                {{productSettings.web_productPage_comments_rules_link.value_web}}
                            </p>

                            <div class="mt-[10px] pb-[10px] [&>*:not(:last-child)]:border-b border-gray-c200">
                                
                                {%for comment in userComments%}
                                    <div>
                                        {%render '/components/main/product/comment',comment=comment,confirmed=false%}
                                    </div>
                                {%endfor%}
                                
                                {%for comment in product.comments%}
                                    <div>
                                        {%render '/components/main/product/comment', comment=comment,confirmed=true%}
                                    </div>
                                {%endfor%}

                                <!-- <button class="text-blue-c200 font-shabnam-bold text-[14px]">مخفی کردن پاسخ ها
                                    <svg class="!inline" width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M12.3493 10.1588C12.1667 10.3451 11.8809 10.362 11.6796 10.2096L11.6219 10.1588L7.5 5.95411L3.37814 10.1588C3.19552 10.3451 2.90974 10.362 2.70836 10.2096L2.65067 10.1588C2.46804 9.97253 2.45144 9.68102 2.60086 9.4756L2.65067 9.41675L7.13626 4.84119C7.31889 4.6549 7.60466 4.63796 7.80604 4.79038L7.86374 4.84119L12.3493 9.41675C12.5502 9.62166 12.5502 9.9539 12.3493 10.1588Z" fill="#003EA0"/>
                                    </svg>
                                </button>

                                <button class="text-blue-c200 font-shabnam-bold text-[14px] opacity-40 mb-[100px]">مخفی کردن پاسخ ها
                                    <svg class="!inline rotate-180" width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M12.3493 10.1588C12.1667 10.3451 11.8809 10.362 11.6796 10.2096L11.6219 10.1588L7.5 5.95411L3.37814 10.1588C3.19552 10.3451 2.90974 10.362 2.70836 10.2096L2.65067 10.1588C2.46804 9.97253 2.45144 9.68102 2.60086 9.4756L2.65067 9.41675L7.13626 4.84119C7.31889 4.6549 7.60466 4.63796 7.80604 4.79038L7.86374 4.84119L12.3493 9.41675C12.5502 9.62166 12.5502 9.9539 12.3493 10.1588Z" fill="#003EA0"/>
                                    </svg>
                                </button> -->

                            </div>
                        </div>
                    {%endif%}

                </div>
            </div>

            <script>
                let tabsContainer = document.querySelector("#tabs");

                let tabTogglers = tabsContainer.querySelectorAll("a");
                console.log(tabTogglers);

                tabTogglers.forEach(function(toggler) {
                    toggler.addEventListener("click", function(e) {
                        e.preventDefault();

                        let tabName = this.getAttribute("href");
                        addToQuery('tab',tabName.replace('#',''));

                        let tabContents = document.querySelector("#tab-contents");

                        for (let i = 0; i < tabContents.children.length; i++) {

                            tabTogglers[i].parentElement.classList.remove("border-blue-c200", "border-b" ,"text-blue-c200");  tabContents.children[i].classList.remove("hidden");
                            if ("#" + tabContents.children[i].id === tabName) {
                                continue;
                            }
                            tabContents.children[i].classList.add("hidden");

                        }
                        e.target.parentElement.classList.add("border-blue-c200", "border-b-4" ,"text-blue-c200");
                    });
                });

                var default_tab = getQuery('tab');
                if (default_tab) {
                    $(`[href="#${default_tab}"]`).get(0).click();
                } else {
                    $(`#tabs`).find('a').get(0).click();
                }

            </script>
            <!-- end info tabs -->

        </div>
    </div>
    <!-- end home main content -->