            <div class="mt-[40px] 2xl:mt-[60px] lg:mt-[50px] bg-white rounded-[10px] px-[25px] pb-[1px]">
                <!-- Tabs -->
                <ul id="tabs" class="px-1 pt-2 font-shabnam-bold-digit text-[13px] pt-[15px] flex justify-between md:justify-start md:gap-12">
                    {%if product.attribute_groups.length%}
                    <li class="pb-[10px] border-b-4"><a href="#first">مشخصات</a></li>
                    {%endif%}
                    {%if product.summary_web%}
                        <li class="pb-[10px] border-b-4"><a href="#second">معرفی محصول</a></li>
                    {%endif%}
                    {%if product.description_web%}
                        <li class="pb-[10px] border-b-4"><a href="#third">نقد و بررسی ها</a></li>
                    {%endif%}
                    {%if product.has_comment%}
                        <li class="pb-[10px] border-b-4"><a href="#fourth">نظرات</a></li>
                    {%endif%}
                </ul>
                <hr>
                <!-- Tab Contents -->
                <div id="tab-contents">

                    {%if product.attribute_groups.length%}
                        <div id="first" class="hidden mt-[30px] font-shabnam-normal-digit text-[13px] flex">
                                <div class=" mb-[10px] text-gray-c400 w-[30%] lg:w-[20%]">

                                    {%for attribute_group in product.attribute_groups%}
                                        {%for attribute in attribute_group.attributes%}
                                            {%if attribute.attribute_value.value != null%}
                                                <div class="py-[20px] ">{{attribute.attribute.title_web}}:</div>
                                            {%endif%}
                                        {%endfor%}
                                    {%endfor%}                            

                                </div>
                                <div class="w-[70%] text-right lg:w-[80%]">

                                    {%if product.attribute_groups.length%}
                                        {%for attribute_group in product.attribute_groups%}
                                            {%for attribute in attribute_group.attributes%}
                                                {%if attribute.attribute_value.value != null%}
                                                    
                                                    {%if attribute.attribute.type != 3%}
                                                        <div class="py-[20px] border-b border-gray-c200">
                                                            {{attribute.attribute_value.value_web}}
                                                        </div>
                                                    {%else%}
                                                        <div class="py-[20px] border-b border-gray-c200">
                                                            {%if attribute.attribute_value.value | not%}
                                                                ندارد
                                                            {%else%}
                                                                دارد
                                                            {%endif%}
                                                        </div>
                                                    {%endif%}
                                                    
                                                {%endif%}
                                            {%endfor%}
                                        {%endfor%}
                                    {%endif%}

                                </div>
                        </div>
                    {%endif%}
                    
                    {%if product.summary_web%}
                        <div id="second" class="hidden my-[30px] font-shabnam-normal-digit text-[16px]">
                            <div class="text-justify">
                                {{product.summary_web}}
                            </div>
                        </div>
                    {%endif%}

                    {%if product.description_web%}
                        <div id="third" class="hidden my-[30px]">
                            <div class="product_texteditor">
                                {{product.description_web}}
                            </div>
                        </div>
                    {%endif%}

                    {%if product.has_comment%}
                        <div id="fourth" class="hidden mt-[30px]">
                            <!-- start comment box -->
