
<script>
    function getCurrentMixVariantKeys() {
        const keys = [];
        const inputs = $('.variant_inputs[checked]').map(function (i, input) {
            keys.push($(input).data('key'));
        });
        return keys;
    }
    function updatePrices() {
        const keys = getCurrentMixVariantKeys();

        $('.variant_prices').addClass('d-none');
        $('.variant_prices').filter(`[data-keys='${keys}']`).removeClass('d-none');
    }
    function updateMediaGallery() {
        const keys = getCurrentMixVariantKeys();

        $('.product_media_gallery').addClass('d-none');
        const target_element = $('.product_media_gallery').filter(`[data-keys='${keys}']`);
        if (target_element.length)
            target_element.removeClass('d-none');
        else
            $('.product_media_gallery').filter(`#main_media_gallery`).removeClass('d-none');
    }
    function updateProductDetails() {
        const keys = getCurrentMixVariantKeys();

        let target_element;

        $('.product_details').addClass('d-none');
        target_element = $('.product_details').filter(`[data-keys='${keys}']`);
        if (target_element.length)
            target_element.removeClass('d-none');
        else
            $('.product_details').filter(`#main_details`).removeClass('d-none');
    }
    $(function () {
        $('.variant_inputs').on('click', function (e) {
            $(`.variant_inputs[name='${this.name}']`).attr('checked', false)
            $(this).attr('checked', true);
            const p = new URL(window.location)?.searchParams;
            let variants = [];
            try {
                const query_variants = p.get('variants');
                if (query_variants)
                    variants = query_variants.split(',');
            } catch {/* empty */ }
            if (variants) {
                let found = false;
                for (let i in variants) {
                    const variant_id = variants[i].split('-')[0];
                    if (variant_id == this.name) {
                        variants[i] = this.id;
                        found = true;
                        break;
                    }
                }
                if (!found) {
                    variants.push(this.id);
                }
            } else {
                variants = [this.id];
            }
            p.set('variants', String(variants));
            window.history.replaceState(null, null, `?${p.toString()}`);
            updatePrices();
            updateMediaGallery();
            updateProductDetails();
        });
    });
    $(function () {
        const p = (new URL(window.location)).searchParams;
        let variants = [];
        try { variants = p.get('variants').split(','); } catch {/* empty */ }
        for (let i in variants) {
            try {
                const variant_id = variants[i].split('-')[0];
                $(`.variant_inputs[name='${variant_id}']`).attr('checked', false)
                $(`#${variants[i]}`).attr('checked', true);
            } catch { /* empty */ }
        }
        updatePrices();
        updateMediaGallery();
        updateProductDetails();
    })
</script>

