            <div class="swiper product-swiper2 w-[92%]">
                <div class="swiper-wrapper">

                    {% for image in product.media_gallery %}
                        <div class="swiper-slide">
                            <div class="group" style="position: relative;">
                                <img class="block w-full h-[400px] lg:h-[350px] 1230:my-[15px] object-contain" src="{{ image.url | abs_url }}" alt="{{ image.alt }}"/>
                                <div class="majid_zoom_background group-hover:block hidden w-full h-[400px] lg:h-[350px]" style="position: absolute;top: 0;background-size: 200%; background-image: url('{{ image.url | abs_url }}');"></div>
                            </div>
                        </div>
                    {% endfor %}

                </div>
            </div>
        </div>
    </div>
    <div thumbsSlider="" class="swiper product-swiper mt-[15px] lg:mt-[20px]">
        <div class="swiper-button-next text-gray-c600 after:text-[15px] bg-white rounded-[5px] h-[27px] top-[60%]"></div>
        <div class="swiper-button-prev text-gray-c600 after:text-[15px] bg-white rounded-[5px] h-[27px] top-[60%]"></div>
        <div class="swiper-wrapper !thumbSwiperTransform lg:lg-thumbSwiperTransform 2xl:twoXl-thumbSwiperTransform">

            {% for image in product.media_gallery %}
                <div class="swiper-slide flex justify-center !ml-0 lg:ml-thumbs lg:w60 xl:w82 2xl:w100 cursor-pointer">
                    <img src="{{ image.url | abs_url }}" alt="{{ image.alt }}" class="rounded-[10px] md:w-[70px]"/>
                </div>
            {% endfor %}

        </div>
    </div>
    <script>
        $(document).ready(() => {
            var swiper = new Swiper(".product-swiper", {
                spaceBetween: 10,
                slidesPerView: 4,
                freeMode: true,
                watchSlidesProgress: true,
            });
            var swiper2 = new Swiper(".product-swiper2", {
                loop: true,
                spaceBetween: 10,
                navigation: {
                    nextEl: ".swiper-button-next",
                    prevEl: ".swiper-button-prev",
                },
                thumbs: {
                    swiper: swiper,
                },
            });
        });
    </script>
